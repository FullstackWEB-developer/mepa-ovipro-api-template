{
    "name": "ovipro",
    "version": "0.1.0",
    "engines": {
        "node": ">=14.16.0 <15.0.0",
        "npm": ">=6.14.11 <7.0.0"
    },
    "bin": {
        "infra": "bin/app.js"
    },
    "scripts": {
        "build": "npx tsc",
        "build:all": "npm run clean && npm run build:lambda:api-model && tsc && npm run build:application && tsc",
        "watch": "node ./node_modules/typescript/bin/tsc -w",
        "lint:es": "eslint '*/**/*.{ts,tsx}' --max-warnings 0",
        "cdk": "npx cdk",
        "test": "npx tsc && jest --runInBand",
        "test:update": "jest --runInBand -u",
        "test:watch": "jest --watch",
        "integration-test": "jest -c jest.config.it.js --runInBand",
        "clean": "npx rimraf '+(bin|lib|functions|__test__|utils)/**/+(*.d.ts|*.js)'",
        "build:application": "concurrently 'npm:build:lambda'",
        "build:lambda": "npm run build:lambda:api-model && concurrently 'npm:build:lambda:java'",
        "build:lambda:api-model": "(npm list @openapitools/openapi-generator-cli --prefix ../api-specs || npm i --prefix ../api-specs) && npm run build:out --prefix ../api-specs -- -o ../infra/functions/api/generated/api-schema.ts",
        "build:lambda:java": "concurrently 'npm:build:lambda:java:*'",
        "build:lambda:java:database:aurora:migrator": "./functions/database/aurora/migrator/build-with-docker.sh"
    },
    "devDependencies": {
        "@almamedia/cdk-accounts-and-environments": "^0.7.0",
        "@almamedia/cdk-assumed-credentials-provider": "^0.4.0",
        "@almamedia/cdk-cross-region-parameter": "^0.1.1",
        "@almamedia/cdk-dynamodb-export": "^0.4.0",
        "@almamedia/cdk-open-api": "^0.11.0",
        "@almamedia/cdk-regions": "^0.1.0",
        "@almamedia/cdk-tag-and-name": "^1.3.0",
        "@aws-cdk/assert": "1.138.2",
        "@aws-cdk/assets": "1.138.2",
        "@aws-cdk/aws-apigateway": "1.138.2",
        "@aws-cdk/aws-apigatewayv2": "1.138.2",
        "@aws-cdk/aws-certificatemanager": "1.138.2",
        "@aws-cdk/aws-cloudformation": "1.138.2",
        "@aws-cdk/aws-cloudfront": "1.138.2",
        "@aws-cdk/aws-cloudfront-origins": "1.138.2",
        "@aws-cdk/aws-cloudwatch": "1.138.2",
        "@aws-cdk/aws-cloudwatch-actions": "1.138.2",
        "@aws-cdk/aws-codedeploy": "1.138.2",
        "@aws-cdk/aws-ec2": "1.138.2",
        "@aws-cdk/aws-ecr-assets": "1.138.2",
        "@aws-cdk/aws-ecs-patterns": "1.138.2",
        "@aws-cdk/aws-elasticache": "1.138.2",
        "@aws-cdk/aws-elasticloadbalancingv2": "1.138.2",
        "@aws-cdk/aws-events": "1.138.2",
        "@aws-cdk/aws-events-targets": "1.138.2",
        "@aws-cdk/aws-iam": "1.138.2",
        "@aws-cdk/aws-kms": "1.138.2",
        "@aws-cdk/aws-lambda": "1.138.2",
        "@aws-cdk/aws-lambda-event-sources": "1.138.2",
        "@aws-cdk/aws-lambda-nodejs": "1.138.2",
        "@aws-cdk/aws-logs": "1.138.2",
        "@aws-cdk/aws-rds": "1.138.2",
        "@aws-cdk/aws-route53": "1.138.2",
        "@aws-cdk/aws-route53-targets": "1.138.2",
        "@aws-cdk/aws-s3": "1.138.2",
        "@aws-cdk/aws-sns": "1.138.2",
        "@aws-cdk/aws-sns-subscriptions": "1.138.2",
        "@aws-cdk/aws-sqs": "1.138.2",
        "@aws-cdk/aws-ssm": "1.138.2",
        "@aws-cdk/aws-wafv2": "1.138.2",
        "@aws-cdk/cloud-assembly-schema": "1.138.2",
        "@aws-cdk/core": "1.138.2",
        "@aws-cdk/custom-resources": "1.138.2",
        "@aws-cdk/cx-api": "1.138.2",
        "@types/aws-lambda": "^8.10.76",
        "@types/jest": "^26.0.23",
        "@types/lodash": "^4.14.168",
        "@types/node": "14.17.4",
        "@types/test-console": "^1.1.0",
        "@typescript-eslint/eslint-plugin": "^4.14.2",
        "@typescript-eslint/parser": "^4.14.2",
        "aws-cdk": "1.138.2",
        "change-case": "^4.1.2",
        "concurrently": "^5.3.0",
        "esbuild": "^0.13.3",
        "eslint": "^7.19.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-prettier": "^3.4.0",
        "husky": "^4.3.8",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.3",
        "pg": "^8.7.1",
        "prettier": "^2.3.0",
        "source-map-support": "^0.5.19",
        "test-console": "^2.0.0",
        "testcontainers": "^7.21.0",
        "ts-jest": "^26.5.5",
        "ts-node": "^9.0.0",
        "typescript": "^4.4.3"
    },
    "dependencies": {
        "@almamedia/ovipro-common-entities": "^0.1.19",
        "@dazn/lambda-powertools-correlation-ids": "1.28.1",
        "@dazn/lambda-powertools-logger": "1.28.1",
        "@dazn/lambda-powertools-middleware-correlation-ids": "1.28.1",
