{
    "name": "ovipro",
    "version": "0.1.0",
    "engines": {
        "node": ">=14.16.0 <15.0.0",
        "npm": ">=6.14.11 <7.0.0"
    },
    "bin": {
        "infra": "bin/app.js"
    },
    "config": {
        "lambda_build_prefix": "npx @vercel/ncc@0.27.0 build --source-map --minify functions",
        "lambda_build_suffix": "-o dist/functions"
    },
    "scripts": {
        "build": "npx tsc",
        "build:all": "npm run clean && npm run build:lambda:api-model && tsc && npm run build:application && tsc",
        "watch": "node ./node_modules/typescript/bin/tsc -w",
        "lint:es": "eslint '*/**/*.{ts,tsx}' --max-warnings 0",
        "cdk": "npx cdk",
        "test": "npx tsc && jest --runInBand",
        "test:update": "jest --runInBand -u",
        "test:watch": "jest --watch",
        "clean": "npx rimraf '+(bin|lib|functions|__test__|utils)/**/+(*.d.ts|*.js)'",
        "build:application": "concurrently 'npm:build:lambda'",
        "build:lambda": "npm run build:lambda:api-model && concurrently 'npm:build:lambda:java' 'npm:build:lambda:ts'",
        "build:lambda:api-model": "(npm list @openapitools/openapi-generator-cli --prefix ../api-specs || npm i --prefix ../api-specs) && npm run build:out --prefix ../api-specs -- -o ../infra/functions/api/generated/api-schema.ts",
        "build:lambda:java": "concurrently 'npm:build:lambda:java:*'",
        "build:lambda:java:database:aurora:migrator": "./functions/database/aurora/migrator/build-with-docker.sh",
        "build:lambda:ts": "concurrently 'npm:build:lambda:ts:*'",
        "build:lambda:ts:stack-lifecycle-event-callback-invoker": "$npm_package_config_lambda_build_prefix/resource-event-callback-invoker/index.ts $npm_package_config_lambda_build_suffix/resource-event-callback-invoker/index",
        "build:lambda:ts:sample": "$npm_package_config_lambda_build_prefix/sample/api/handler/index.ts $npm_package_config_lambda_build_suffix/sample/api/handler"
    },
    "devDependencies": {
        "@almamedia/cdk-cross-region-parameter": "^0.1.1",
        "@almamedia/cdk-regions": "^0.1.0",
        "@almamedia/cdk-dynamodb-export": "^0.4.0",
        "@almamedia/cdk-assumed-credentials-provider": "^0.4.0",
        "@almamedia/cdk-open-api": "^0.9.0",
        "@almamedia/cdk-tag-and-name": "^1.3.0",
        "@almamedia/cdk-accounts-and-environments": "^0.7.0",
        "@aws-cdk/assert": "1.110.0",
        "@aws-cdk/assets": "1.110.0",
        "@aws-cdk/aws-apigateway": "1.110.0",
        "@aws-cdk/aws-apigatewayv2": "1.110.0",
        "@aws-cdk/aws-certificatemanager": "1.110.0",
        "@aws-cdk/aws-cloudwatch": "1.110.0",
        "@aws-cdk/aws-cloudwatch-actions": "1.110.0",
        "@aws-cdk/aws-cloudfront": "1.110.0",
        "@aws-cdk/aws-cloudformation": "1.110.0",
        "@aws-cdk/aws-cloudfront-origins": "1.110.0",
        "@aws-cdk/aws-codedeploy": "1.110.0",
        "@aws-cdk/custom-resources": "1.110.0",
        "@aws-cdk/aws-ec2": "1.110.0",
        "@aws-cdk/aws-ecr-assets": "1.110.0",
        "@aws-cdk/aws-ecs-patterns": "1.110.0",
        "@aws-cdk/aws-elasticache": "1.110.0",
        "@aws-cdk/aws-elasticloadbalancingv2": "1.110.0",
        "@aws-cdk/aws-events": "1.110.0",
        "@aws-cdk/aws-events-targets": "1.110.0",
        "@aws-cdk/aws-iam": "1.110.0",
        "@aws-cdk/aws-lambda": "1.110.0",
        "@aws-cdk/aws-lambda-event-sources": "1.110.0",
        "@aws-cdk/aws-logs": "1.110.0",
        "@aws-cdk/aws-rds": "1.110.0",
        "@aws-cdk/aws-route53": "1.110.0",
        "@aws-cdk/aws-route53-targets": "1.110.0",
        "@aws-cdk/aws-sns": "1.110.0",
        "@aws-cdk/aws-sns-subscriptions": "1.110.0",
        "@aws-cdk/aws-sqs": "1.110.0",
        "@aws-cdk/aws-ssm": "1.110.0",
        "@aws-cdk/aws-wafv2": "1.110.0",
        "@aws-cdk/core": "1.110.0",
        "@types/aws-lambda": "^8.10.76",
        "@types/jest": "^26.0.23",
        "@types/node": "14.17.4",
        "@typescript-eslint/eslint-plugin": "^4.14.2",
        "@typescript-eslint/parser": "^4.14.2",
        "aws-cdk": "1.110.0",
        "change-case": "^4.1.2",
        "concurrently": "^5.3.0",
        "eslint": "^7.19.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-prettier": "^3.4.0",
        "husky": "^4.3.8",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.3",
        "prettier": "^2.3.0",
        "source-map-support": "^0.5.19",
        "ts-jest": "^26.5.5",
        "ts-node": "^9.0.0",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "@middy/core": "~2.4.0",
        "@middy/http-error-handler": "~2.4.0",
        "@middy/http-json-body-parser": "~2.4.0",
        "@middy/validator": "~2.4.0",
        "@types/http-errors": "^1.8.0",
        "@types/lodash": "^4.14.170",
        "@types/uuid": "^8.3.0",
        "aws-sdk": "^2.854.0",
        "aws-xray-sdk": "^3.2.0",
        "cdk-dia": "^0.3.0",
        "http-errors": "^1.8.0",
        "jest-sonar-reporter": "^2.0.0",
        "json-colorizer": "^2.2.2",
        "lambda-log": "^2.4.0",
        "lodash": "^4.17.21",
        "sqlite3": "^5.0.2",
        "typeorm": "0.2.34",
        "typeorm-aurora-data-api-driver": "2.1.1",
        "typeorm-naming-strategies": "2.0.0",
        "typescript-logging": "^1.0.0",
        "uuid": "^8.3.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.ts": "eslint --cache --fix",
        "*.{ts,css,md}": "prettier --write"
    }
}
