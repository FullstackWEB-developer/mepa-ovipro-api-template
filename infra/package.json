{
    "name": "ovipro",
    "version": "0.1.0",
    "engines": {
        "node": ">=14.16.0 <15.0.0",
        "npm": ">=6.14.11 <7.0.0",
        "yarn": "\n\n Use NPM! \n\n"
    },
    "bin": {
        "infra": "bin/app.js"
    },
    "scripts": {
        "build": "npx tsc",
        "build:all": "npm run clean && npm run build:lambda:api-model && tsc && npm run build:application && tsc",
        "watch": "node ./node_modules/typescript/bin/tsc -w",
        "lint:es": "eslint '*/**/*.{ts,tsx}' --max-warnings 0",
        "cdk": "npx cdk",
        "test": "npx tsc && jest --runInBand",
        "integration-test": "jest -c jest.config.it.js --runInBand",
        "it": "npm run integration-test",
        "test:update": "jest --runInBand -u --collect-coverage=false",
        "test:watch": "jest --watch --collect-coverage=false",
        "clean": "npx rimraf '+(bin|lib|functions|__test__|utils)/**/+(*.d.ts|*.js)'",
        "build:application": "concurrently 'npm:build:lambda'",
        "build:lambda": "npm run build:lambda:api-model && concurrently 'npm:build:lambda:java'",
        "build:lambda:api-model": "(npm list @openapitools/openapi-generator-cli --prefix ../api-specs || npm i --prefix ../api-specs) && npm run build:out --prefix ../api-specs -- -o ../infra/functions/api/generated/api-schema.ts",
        "build:lambda:java": "concurrently 'npm:build:lambda:java:*'",
        "build:lambda:java:database:aurora:migrator": "./functions/database/aurora/migrator/build-with-docker.sh"
    },
    "devDependencies": {
        "@almamedia-open-source/cdk-cross-region-parameter": "./demo-packages/cdk-cross-region-parameter@0.0.0.jsii.tgz",
        "@almamedia-open-source/cdk-openapix": "./demo-packages/cdk-openapix@0.0.20.jsii.tgz",
        "@almamedia-open-source/cdk-project-context": "0.0.19",
        "@almamedia-open-source/cdk-project-names": "0.0.8",
        "@almamedia-open-source/cdk-project-stack": "0.0.5",
        "@almamedia-open-source/cdk-project-target": "0.0.10",
        "@aws-sdk/client-sfn": "3.51.0",
        "@types/aws-lambda": "^8.10.76",
        "@types/jest": "^26.0.23",
        "@types/lodash": "^4.14.168",
        "@types/node": "14.17.4",
        "@types/test-console": "^1.1.0",
        "@typescript-eslint/eslint-plugin": "^5",
        "@typescript-eslint/parser": "^5",
        "aws-cdk": "2.16.0",
        "aws-sdk-mock": "^5.1.0",
        "change-case": "^4.1.2",
        "concurrently": "^5.3.0",
        "esbuild": "^0.13.3",
        "eslint": "^8",
        "eslint-import-resolver-node": "^0.3.6",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.25.4",
        "husky": "^4.3.8",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.3",
        "msw": "^0.36.5",
        "pg": "^8.7.1",
        "source-map-support": "^0.5.19",
        "test-console": "^2.0.0",
        "testcontainers": "^8.6.1",
        "ts-jest": "^26.5.5",
        "ts-node": "^9.0.0",
        "typescript": "^4.4.3"
    },
    "peerDependencies": {
        "aws-cdk-lib": "^2.11.0",
        "constructs": "^10.0.0"
    },
    "dependencies": {
        "@almamedia/ovipro-common-entities": "^0.1.21",
        "@dazn/lambda-powertools-correlation-ids": "1.28.1",
        "@dazn/lambda-powertools-logger": "1.28.1",
        "@dazn/lambda-powertools-middleware-correlation-ids": "1.28.1",
        "@middy/core": "2.4.0",
        "@middy/http-error-handler": "2.2.0",
        "@middy/http-json-body-parser": "2.2.0",
        "@middy/validator": "2.2.0",
        "@middy/util": "^2.4.3",
        "@opensearch-project/opensearch": "^1.0.2",
        "@types/http-errors": "^1.8.0",
        "@types/uuid": "^8.3.0",
        "aws-sdk": "^2.839.0",
        "aws-xray-sdk": "^3.2.0",
        "axios": "^0.21.4",
        "http-errors": "^1.8.0",
        "jest-sonar-reporter": "^2.0.0",
        "json-colorizer": "^2.2.2",
        "lambda-log": "^2.4.0",
        "lodash": "^4.17.21",
        "openapi-request-validator": "^9.0.0",
        "typeorm": "0.2.37",
        "typeorm-aurora-data-api-driver": "2.3.1",
        "typeorm-naming-strategies": "2.0.0",
        "typescript-logging": "^1.0.0",
        "uuid": "^8.3.2",
        "aws-cdk-lib": "2.16.0",
        "constructs": "^10.0.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.ts": "eslint --cache --fix"
    }
}
